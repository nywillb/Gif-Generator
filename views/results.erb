  <body>

    <div class="site-wrapper">

      <div class="site-wrapper-inner">

        <div class="cover-container">

          <div class="masthead clearfix">
            <div class="inner">
              <a href="/"><h3 class="masthead-brand">Gif Time</h3></a>
            </div>
          </div>

          <div class="inner cover">
            <h1 class="cover-heading">GIF your life.</h1>
            <p class="lead">Translate text into GIFs to <strong>GIF your life.</strong></p>
            <p class="lead">
              <%@resultUrls.each do |gif|%>
                <%if !gif.start_with?('http://i.giphy.com/')%>
                <%=gif.to_s%>
                <%else%>
                <img src=<%=gif.to_s%> height=125px>
                <%end%>
              <%end%>
            </p>
          </div>

          <div class="mastfoot">
            <div class="inner">
            </div>
          </div>

        </div>

      </div>

    </div>

